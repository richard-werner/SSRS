    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>RUGS_IMS</DataSourceName>
        <CommandText>Select year(date) sales_yr
, month(date) sales_mth
, DATENAME(month,date) sale_mnt_name
, sum(total_qty) Sales_MTD_ALL
, sum(case when location not like 'Not%' then total_qty else 0 end) Sales_MTD_NonDI
, isnull(rec_qty,0)+isnull(pending.Shipping_Quantity,0) Global_rec_QTY
from dbo.tbhistorder ho
left join (
		select year(estimateD_delivery_date) rec_yr, month(estimateD_delivery_date) rec_mth, sum(shipping_quantity) rec_qty
		from Rugs_Prod2_test.dbo.po_shipping
		where completed ='Y' and year(estimated_delivery_date) in (year(getdate()),year(dateadd(year, -1,getdate())))
		group by year(estimateD_delivery_date), month(estimateD_delivery_date)
	) ps on ps.rec_yr=year(date) and ps.rec_mth=month(date)
left join (
		select year(getdate()) current_yr,month(getdate()) current_mth, shipping_quantity 
		from dbo.vLiveMTDreceipts
		where status='Core Scanned, Pending Ofbiz Confirm QTY'
		) pending on pending.current_yr=year(date) and pending.current_mth=month(date)
where year(date) in (year(getdate()), year(dateadd(year, -1,getdate())))
group by year(date) ,month(date)
, DATENAME(month,date)
, isnull(rec_qty,0)+isnull(pending.Shipping_Quantity,0)
order by sales_yr, sales_mth</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
